    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="310dp"
        android:layout_marginBottom="8dp"
        android:background="#05FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/petInfo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/bathData"
        app:srcCompat="@drawable/plus"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/cut"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="287dp"
        android:layout_marginBottom="5dp"
        android:background="#05FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/bathData"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/etcData"
        app:srcCompat="@drawable/plus" />

    <TextView
        android:id="@+id/petInfo"
        android:layout_width="333dp"
        android:layout_height="64dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="128dp"
        android:background="@drawable/shadow"
        android:textAlignment="center"
        android:textColor="#949494"
        android:textColorHint="#DFDDDD"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/bathData"
        android:layout_width="333dp"
        android:layout_height="64dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="11dp"
        android:background="@drawable/shadow"
        android:textAlignment="center"
        android:textColor="#949494"
        android:textColorHint="#949494"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageButton3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cut"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/etcData"
        android:layout_width="333dp"
        android:layout_height="64dp"
        android:layout_marginTop="13dp"
        android:background="@drawable/shadow"
        android:textAlignment="center"
        android:textColor="#949494"
        android:textColorHint="#949494"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/cut"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/cutData"
        android:layout_width="317dp"
        android:layout_height="153dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="11dp"
        android:background="@drawable/shadow"
        android:textAlignment="center"
        android:textColor="#949494"
        android:textColorHint="#949494"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="62dp"
        android:layout_height="55dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toTopOf="@+id/cutData"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="268dp"
        android:layout_marginBottom="40dp"
        android:text="펫정보"
        app:layout_constraintBottom_toTopOf="@+id/cutData"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="2dp"
        android:text="목욕"
        app:layout_constraintBottom_toTopOf="@+id/etcData"
        app:layout_constraintEnd_toStartOf="@+id/cutData"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cutData" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="5dp"
        android:text="발톱깍기"
        app:layout_constraintBottom_toTopOf="@+id/bathData"
        app:layout_constraintEnd_toStartOf="@+id/cut"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etcData" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="39dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="18dp"
        android:text="미용"
        app:layout_constraintBottom_toTopOf="@+id/petInfo"
        app:layout_constraintEnd_toStartOf="@+id/imageButton3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bathData" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="11dp"
        android:layout_marginBottom="7dp"
        android:background="#05FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/etcData"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.019"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/cutData"
        app:srcCompat="@drawable/plus" />


</androidx.constraintlayout.widget.ConstraintLayout>